{
  "name": "feedz",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node-dev -r tsconfig-paths/register --respawn src/index.ts",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration --runInBand --forceExit",
    "test:unit": "jest --testPathPattern=unit --coverage --watch",
    "test:e2e": "jest --testPathPattern=e2e --runInBand --detectOpenHandles --watch-all --forceExit",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api",
    "mysql",
    "prisma",
    "zod"
  ],
  "author": "Henrique Martins Alexandre, Vitor Pagani Vieira, Anderson do Carmo Ramos & St√©fany Isabelle",
  "license": "MIT",
  "description": "Backend of Feedz Project",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.13.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.2",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ]
}
